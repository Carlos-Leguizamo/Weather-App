{
  "expo": {
    "name": "Weather App",
    "slug": "Weather-App",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/ice-cream.png",
    "splash": {
      "image": "./assets/images/HELADOS.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "REPLACE_FOR_API_KEY"
      }
    },
    "android": {
      "package": "com.usco.weather",
      "versionCode": 2,
      "permissions": [
        "INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ice-cream.png",
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCPwMeGSJCK4fhXpPkLcWNpW9XD5oJUQ3E"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "config": {
        "googleMaps": {
          "apiKey": "REPLACE_FOR_API_KEY"
        }
      }
    },
    "extra": {
      "eas": {
        "projectId": "83d80179-dd76-4833-80fe-c79da82b3fb1"
      }
    }
  }
}